  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/canada-ca/aia-eia"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:md": "markdownlint \"**/*.md\" -i \"node_modules\"",
    "spellcheck:markdown": "cspell **/*.md --color",
    "spellcheck:code": "cspell src/**/*.ts src/**/*.vue --color",
    "spellcheck:questions": "cspell src/plugins/*.json src/survey-enfr.json --color",
    "deploy": "npm run build; push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@types/lodash.isempty": "^4.4.6",
    "@types/showdown": "^1.9.3",
    "bootstrap-vue": "^2.19.0",
    "lodash.isempty": "^4.4.0",
    "register-service-worker": "^1.7.1",
    "showdown": "^1.9.1",
    "survey-vue": "^1.8.16",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.22.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "cspell": "^4.2.3",
    "cspell-dict-fr-fr": "^1.2.17",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-vue": "^7.1.0",
    "markdownlint-cli": "^0.25.0",
    "prettier": "^2.2.0",
    "push-dir": "^0.4.1",
    "typescript": "^4.1.2",
    "vue-template-compiler": "^2.6.12"
  }
}
